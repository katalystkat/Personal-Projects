color_choice = OCEAN
object_trace = MONSTERA

class Dots(Turtle):
    def __init__(self):
        super().__init__()
        self.new_dot = None
        self.speed(0)

    def new_dots_a(self, color_a, x, y):
        self.new_dot = Turtle(shape="circle")
        self.new_dot.penup()
        self.new_dot.setposition(x, y)
        random_stretch = random.uniform(0.3, 0.6)
        self.new_dot.shapesize(random_stretch, random_stretch)
        self.new_dot.color(color_a)

    def new_dots_b(self, color_b, x, y):
        self.new_dot = Turtle(shape="circle")
        self.new_dot.penup()
        self.new_dot.setposition(x, y)
        new_shapesize = random.uniform(0.05, 0.2)
        self.new_dot.shapesize(new_shapesize, new_shapesize)
        self.new_dot.color(color_b)


    def new_dots_pair(self, object_trace, color_choice, object_range):
        x = 0
        y = 0
        for x in range(0, object_range-1):
            x +=1
            y +=1
            x_position = object_trace[x][0] - 200
            y_position = object_trace[y][1] - 600
            random_color_pair_a = color_choice[random.randrange(0, 9)]
            random_color_pair_b = color_choice[random.randrange(0, 9)]
            self.new_dots_a(color_a=random_color_pair_a, x=x_position, y=y_position)
            self.new_dots_b(color_b=random_color_pair_b, x=x_position, y=y_position)

    # PRetty sure we can scratch the below
    def start_dots(self):
        random_color_pair_a = color_choice[random.randrange(0, 9)]
        random_color_pair_b = color_choice[random.randrange(0, 9)]
        self.new_dots_a(color_a=random_color_pair_a, x=0, y=0)
        self.new_dots_b(color_b=random_color_pair_b, x=0, y=0)
        # self.new_dots_a(color_a=random_color_pair_a, x=random_x, y=random_y)
        # self.new_dots_b(color_b=random_color_pair_b, x=random_x, y=random_y)
